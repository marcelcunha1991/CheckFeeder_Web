<%- include ('../partials/header.ejs') %>

<div class="container">

  
 
  <br>
    <div class="card">
            <div class="card-header mx-auto">
                   
                    <h2>Realimentação - <%=maquina%></h2>
                    
            </div>
            <div class="card-body">

                            <input type="hidden" id="maquina" value="<%= maquina%>"> 
                            <input type="hidden" id="posicoes" value="<%= posicoes%>">  
              
                                
                                <input class="form-control" type="text" name="posicao" id="posicao" placeholder="Posição" ><br>
                               
                          
                                <label for="produtoEsperado" id="produtoEsperado" value=""></label>
                            <input class="form-control" type="text" name="produto" id="produto" placeholder="Produto" required><br>
                            <input class="form-control" type="text" name="quantidade" id="quantidade" placeholder="Quantidade"><br>   
                            <input class="form-control" type="text" name="reelid" id="reelid" placeholder="Reel ID"><br>                      
                                                    

                            <div class="row">
                                <button class="btn btn-primary">Realimentar</button>                                
                                <button id="btnVoltar" style= "padding-left: 12px; padding-right: 12px;" class="btn btn-primary">Voltar</button>       
                                <button id="btnLimpar" class="btn btn-primary">Limpar</button>
                                <h2 id="contador" style="position: absolute; right: 5%;"> 1 </h2>                                                  
                            </div>
                
                           
                        
            </div>
           
          
    </div>
   
</div>

<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<script src=/scripts/RealimentacaoScript.js></script>
<%- include ('../partials/footer.ejs') %>